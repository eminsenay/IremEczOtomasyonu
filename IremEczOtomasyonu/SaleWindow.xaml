        xmlns:my="clr-namespace:IremEczOtomasyonu" 
        Title="Satış Ekranı" Height="450" Width="900" Closed="Window_Closed">
    <Grid Name="saleGrid">
        <my:UserControlSales x:Name="userControlSales" Margin="0,0,0,35" />
        <Button Content="Tamam" HorizontalAlignment="Right" Margin="0,0,93,6" Name="okButton" Width="75" Height="23" VerticalAlignment="Bottom" Click="OkButton_Click" />
        <Button Content="İptal" HorizontalAlignment="Right" Margin="0,0,12,6" Name="cancalButton" Width="75" Height="23" VerticalAlignment="Bottom" IsCancel="True" />
    </Grid>
</Window>
